{
    "name": "aaronbullard/eads",
    "description": "PHP Utility for EADS APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Bullard",
            "email": "aaron.bullard77@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EADS\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always --coverage-html=./coverage --coverage-text=tests/summary.txt"
    }
}
